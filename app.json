{
  "expo": {
    "name": "rn-base-app",
    "slug": "rn-base-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#A11327"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adhiraj23zelthy.rnbaseapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adhiraj23zelthy.rnbaseapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "15b5a1db-e63f-465a-b17a-3b46ce5e1d93"
      }
    }
  }
}
